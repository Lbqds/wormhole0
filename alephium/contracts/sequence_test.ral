import "sequence.ral"

TxContract SequenceTest(mut initialized: Bool, mut sequenceAddress: ByteVec) {
    pub fn init(address: ByteVec) -> () {
        assert!(!initialized)
        initialized = true
        let sequence = Sequence(address)
        assert!(sequence.getOwner() == selfContractId!())
        sequenceAddress = address
    }

    pub fn check(seq: U256) -> () {
        assert!(initialized)
        let sequence = Sequence(sequenceAddress)
        sequence.checkSequence(seq)
    }
}
